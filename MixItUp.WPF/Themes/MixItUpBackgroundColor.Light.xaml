<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:MixItUp.WPF.Themes">

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="MixItUpTheme.Common.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <SolidColorBrush x:Key="UserStreamerRoleColor">#FF000000</SolidColorBrush>
    <SolidColorBrush x:Key="UserStaffRoleColor">#FFFFD700</SolidColorBrush>
    <SolidColorBrush x:Key="UserGlobalModRoleColor">#FF07FDC6</SolidColorBrush>
    <SolidColorBrush x:Key="UserModRoleColor">#FF008000</SolidColorBrush>
    <SolidColorBrush x:Key="UserProRoleColor">#FF800080</SolidColorBrush>
    <SolidColorBrush x:Key="UserDefaultRoleColor">#FF0000FF</SolidColorBrush>

</ResourceDictionary>